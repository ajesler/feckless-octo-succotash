{
  "manifest_version": 2,
  "name": "Jenkins Job Git Branch Manager",
  "description": "Provides a quick way to change the git branch of a Jenkins job or set of jobs and to trigger a build.",
  "version": "0.1",
  "homepage_url": "https://github.com/ajesler/jenkins-job-git-branch-extension",
  "icons": { "48": "images/icon48.png" },
  "browser_action": {
    "default_icon": "images/icon48.png",
    "default_popup": "popup.html",
    "default_title": "Jenkins Job Git Branch Manager"
  },
  "permissions": [
    "storage",
    "https://*/",
    "http://*/"
  ],
  "options_page": "options.html",
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  }
}
